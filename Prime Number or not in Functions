def prime(num):
    if num>2:
        flag=0
        for i in range(2,num):
            flag=0
            if num%i==0:
                flag=0
                break
            else:
                flag=1
    if flag==0:
        print("it is not a prime number")
    else:
        print("it is a prime number")
print(prime(5))
